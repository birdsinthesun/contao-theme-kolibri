{
  "name": "birdsinthesun/contao-theme-kolibri",
  "type": "contao-theme",
  "homepage": "https://birdsinthesun.de/kolibri-theme",
  "version": "1.0.0",
  "license": "GPL",
  "require": {
    "contao/calendar-bundle": "5.5.*",
    "contao/comments-bundle": "5.5.*",
    "contao/conflicts": "*@dev",
    "contao/faq-bundle": "5.5.*",
    "contao/manager-bundle": "5.5.*",
    "contao/news-bundle": "5.5.*",
    "contao/newsletter-bundle": "5.5.*",
     "twig/intl-extra":"3.20.*"
  },
  "extra": {
    "contao-component-dir": "assets"
  },
  "scripts": {
    "post-install-cmd": [
      "@php vendor/bin/contao-setup"
    ],
    "post-update-cmd": [
      "@php vendor/bin/contao-setup"
    ]
  }
}
